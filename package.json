{
  "name": "incicle-test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "data": "json-server ./src/server/data.json --port 3333",
    "management": "json-server ./src/server/management.json --port 3334",
    "start": "concurrently \"npm:dev\"  \"npm:data\" \"npm:management\" --kill-others-on-fail"
  },
  "dependencies": {
    "axios": "0.27.2",
    "concurrently": "7.2.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "sass": "1.52.1"
  },
  "devDependencies": {
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "@vitejs/plugin-react": "1.3.0",
    "json-server": "0.17.0",
    "typescript": "4.6.3",
    "vite": "2.9.9"
  }
}
